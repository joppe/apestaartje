{
  "name": "@apestaartje/geometry",
  "version": "1.5.0",
  "description": "A library with functions that help you with geometry",
  "scripts": {
    "build": "tsc --project tsconfig.build.json --outDir dist",
    "dev": "tsc --watch --project ./tsconfig.build.json",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "eslint ./src --fix",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": {
    "name": "Joppe Aarts",
    "email": "joppe@apestaartje.info"
  },
  "license": "MIT",
  "keywords": [
    "TypeScript"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@apestaartje/eslint-config": "*",
    "@apestaartje/ts-config": "*",
    "@types/jest": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
